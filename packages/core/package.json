{
  "name": "@monkey-agent/core",
  "version": "0.1.0",
  "description": "Monkey Agent 核心框架",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:unit": "vitest run src/compression/__tests__/unit",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:weather": "yarn dlx tsx src/__tests__/test_weather_agent.ts",
    "test:llm": "yarn dlx tsx src/__tests__/test_llm_client.ts",
    "test:compression": "yarn dlx tsx src/__tests__/test_context_compression.ts",
    "test:boundary": "yarn dlx tsx src/__tests__/test_message_boundary.ts",
    "test:openrouter": "yarn dlx tsx src/__tests__/test_openrouter.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.70",
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/azure": "^2.0.90",
    "@ai-sdk/deepseek": "^1.0.32",
    "@ai-sdk/google": "^2.0.46",
    "@ai-sdk/google-vertex": "^3.0.92",
    "@ai-sdk/openai": "^2.0.85",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^5.0.112",
    "eventemitter3": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "vitest": "^4.0.16"
  }
}
